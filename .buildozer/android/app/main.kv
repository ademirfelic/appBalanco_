#:include kv/homepage.kv
#:include kv/loginpage.kv
#:include kv/ajustepage.kv
#:include kv/camerapage.kv

#:import utils kivy.utils

FloatLayout:
    canvas:
        Rectangle:
            size: self.size
            pos: self.pos
            source:'icones/bg1.png'
    FloatLayout:

        ScreenManager:
            pos_hint:{"right": 1, "top":1}
            size_hint: 1, 1
            id: screen_manager
            LoginPage:
                name:"loginpage"
                id: loginpage
            HomePage:
                name:"homepage"
                id: homepage
            AjustePage:
                name:"ajustepage"
                id:ajustepage
            CameraPage:
                name:"camerapage"
                id:camerapage